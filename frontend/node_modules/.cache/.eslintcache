/ListGroup.Item>\r\n                        <ListGroup.Item>\r\n                           <Rating value={product.rating} text={`${product.numReviews} reviews`} color={'#f8e825'}/>\r\n                        </ListGroup.Item>\r\n\r\n                        <ListGroup.Item>\r\n                            Price : ${product.price}\r\n                        </ListGroup.Item>\r\n\r\n                        <ListGroup.Item>\r\n                            Description : {product.description}\r\n                        </ListGroup.Item>\r\n                        </ListGroup></Col>\r\n                <Col md={3} >\r\n                    <Card>\r\n                        <ListGroup>\r\n                            <ListGroup.Item>\r\n                                <Row>\r\n                                    <Col>Price:</Col>\r\n                                    <Col><strong>${product.price}</strong></Col>\r\n                                </Row>\r\n                                \r\n                            </ListGroup.Item>\r\n                            <ListGroup.Item>\r\n                                <Row>\r\n                                    <Col>Status:</Col>\r\n                                    <Col>{product.countInStock>0 ? 'In Stock' :'Out Stock'}</Col>\r\n                                </Row>\r\n                                </ListGroup.Item>\r\n                                {/* doubt */}\r\n\r\n                                {product.countInStock>0 &&(\r\n                                    <ListGroup.Item>\r\n                                        <Row>\r\n                                            <Col>\r\n                                            Quantity\r\n                                            </Col>\r\n                                            <Col xs='auto' className = 'my-1'>\r\n                                                <Form.Control as='select'\r\n                                                value = {qty}\r\n      